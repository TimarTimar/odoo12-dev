<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--maestres form view -->
    <record id="maestres_form" model="ir.ui.view">
        <field name="name">Name</field>
        <field name="model">maestres.maestres</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>

            </form>
        </field>
    </record>
    <record id="maestres_tree" model="ir.ui.view">
        <field name="name">Name</field>
        <field name="model">maestres.maestres</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="maestres_action" model="ir.actions.act_window">
        <field name="name">Maestres</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">maestres.maestres</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="maestres_menu" name="Maestres" sequence="4" action="maestres_action"></menuitem>
</odoo>